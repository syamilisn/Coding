]0;ee213031@lpz-ee213031: ~/Desktop/Coding/A2_Linux[01;32mee213031@lpz-ee213031[00m:[01;34m~/Desktop/Coding/A2_Linux[00m$ cat script21.sh
pin=1234
#read -p "Enter Login Pin: " login
i=0
while [ $i -eq 3 ]
do
	i=`expr $i + 1`
	read -p "Enter Login Pin: " login

	if [ $login -eq $pin ];then 
		echo "Login success!!"
		break
	else
		echo "Try again!"
		#read -p "Enter Login Pin: " login
	fi
done
